    <title>RoutesHost</title>
	<meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="~/wwwroot/lib/angular-material/angular-material.css">
</head>
<body>
	
	<div id="main" ui-view="main" layout="column"></div>

    <script src="/wwwroot/lib/angular/angular.min.js"></script>
