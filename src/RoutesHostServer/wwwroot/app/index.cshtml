    <script src="/wwwroot/lib/angular-material/angular-material.min.js"></script>
    <script src="/wwwroot/lib/angular-resource/angular-resource.min.js"></script>
    <script src="/wwwroot/lib/angular-cookies/angular-cookies.min.js"></script>
    <script src="/wwwroot/lib/angular-ui-router/release/angular-ui-router.js"></script>
	<script src="/wwwroot/app/index.js"></script>
